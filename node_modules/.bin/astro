#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/c/Users/Federico/carrot-store/node_modules/.pnpm/astro@5.13.3_@types+node@24_3460070d17ef60d55429e68de911e4fa/node_modules/astro/node_modules:/proc/cygdrive/c/Users/Federico/carrot-store/node_modules/.pnpm/astro@5.13.3_@types+node@24_3460070d17ef60d55429e68de911e4fa/node_modules:/proc/cygdrive/c/Users/Federico/carrot-store/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/proc/cygdrive/c/Users/Federico/carrot-store/node_modules/.pnpm/astro@5.13.3_@types+node@24_3460070d17ef60d55429e68de911e4fa/node_modules/astro/node_modules:/proc/cygdrive/c/Users/Federico/carrot-store/node_modules/.pnpm/astro@5.13.3_@types+node@24_3460070d17ef60d55429e68de911e4fa/node_modules:/proc/cygdrive/c/Users/Federico/carrot-store/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/astro@5.13.3_@types+node@24_3460070d17ef60d55429e68de911e4fa/node_modules/astro/astro.js" "$@"
else
  exec node  "$basedir/../.pnpm/astro@5.13.3_@types+node@24_3460070d17ef60d55429e68de911e4fa/node_modules/astro/astro.js" "$@"
fi
